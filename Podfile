platform :ios,’9.0’

# ruby语法
# target数组 如果有新的target直接加入该数组
targetsArray = ['TodayList', 'ExTensionDemo', 'ExTensionDemoTests', 'ExTensionDemoUITests']
# 循环
targetsArray.each do |t|
    target t do
        pod 'FMDB', '~> 2.7.5'
    end
end


